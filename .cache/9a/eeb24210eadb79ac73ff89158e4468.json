: 30px;\r\n    color: #fff;\r\n    border-radius: 5px;\r\n    padding: 20px;\r\n    font-size: 150%;\r\n    text-align: center;\r\n    transition: all 0.2s ease-in-out;\r\n}\r\n\r\n\r\n/* start button */\r\n.start {\r\n    position: fixed;\r\n    top: 100px;\r\n    background-color: #008CBA; /* blue */\r\n    border: none;\r\n    color: white;\r\n    padding: 15px 15px;\r\n    text-align: center;\r\n    text-decoration: none;\r\n    display: inline-block;\r\n    font-size: 16px;\r\n}\r\n.start:hover {\r\n    background-color: #4CAF50; /* green */\r\n}\r\n\r\n/* mode */\r\n.mode {\r\n    position: fixed;\r\n    left: 50px;\r\n    right: 50px;\r\n    display: flex;\r\n    justify-content: space-between;\r\n    z-index: 10;\r\n}\r\n\r\n.mode > a {\r\n    font-size: 1.5em;\r\n    text-decoration: none;\r\n    display: inline-block;\r\n    padding: 8px 16px;\r\n    background: rgba(70, 112, 105, 0.6);\r\n    color: white;\r\n    border-radius: 5px;\r\n}\r\n\r\n\r\n/* index */\r\n.index {\r\n    display: flex;\r\n    position: fixed;\r\n    justify-content: space-between;\r\n    top: 70%;\r\n    left: 20px;\r\n    right: 20px;\r\n}\r\n\r\n@media (max-width: 796px) {\r\n    .index {\r\n        position: fixed;\r\n    }\r\n}\r\n\r\n.index > a {\r\n    font-size: 1.5em;\r\n    height: 100%;\r\n    line-height: 100%;\r\n    text-decoration: none;\r\n    display: inline-block;\r\n    padding: 4px 10px;\r\n    border: 1px solid #444;\r\n    color: black;\r\n    border-radius: 5px;\r\n}\r\n\r\n.index > a:hover {\r\n    background-color: #3b5998;\r\n    color: white;\r\n}\r\n\r\n.index .next,\r\n.index .previous {\r\n    padding-bottom: 6px;\r\n}\r\n\r\n.index.hidden {\r\n    display: none;\r\n}\r\n\r\n\r\n/* scrollbar */\r\n/* width */\r\n\r\n/* Track */\r\n::-webkit-scrollbar {\r\n    background-color: transparent;\r\n    width: 70px;\r\n}\r\n\r\n::-webkit-scrollbar-track {\r\n    box-shadow: inset 0 0 5px white;\r\n    border-radius: 10px;\r\n    max-height: 50%;\r\n    height: 50%;\r\n}\r\n\r\n/* Handle */\r\n::-webkit-scrollbar-thumb {\r\n    background: #ddc0ad;\r\n    border-radius: 10px;\r\n}\r\n\r\n/* Handle on hover */\r\n::-webkit-scrollbar-thumb:hover {\r\n    background: #b36257;\r\n}\r\n\r\n/* tool tips */\r\n.tooltip-message {\r\n}\r\n\r\n.tooltip-output {\r\n    z-index: 100;\r\n    display: none;\r\n    position: absolute;\r\n    border: 1px solid #ddd;\r\n    background-color: #444;\r\n    color: white;\r\n    padding: 5px;\r\n    top: 100px;\r\n    left: 50px;\r\n}\r\n\r\n\r\n\r\n\r\n\r\n\r\n"},"lineCount":null}},"error":null,"hash":"887fd5a8fc5a3414ed83db4ef556d0a2","cacheData":{"env":{}}}